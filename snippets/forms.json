    "TcSS 1.0.0 Form Elements - Input":{
        "prefix": "t--frm-inp",
        "body": [

            "\t<div class=\"frmElm_${1}\">",
                "$0",
                "\t\t<label  for=\"\"> Demo label </label>",
                "\t\t<input  type=\"text\">",
                "$0",
            "\t</div>"

        ],
        "description": "Creates an input element."
    },



