{
    "TcSS 1.0.0 Form Elements - Input":{
        "prefix": "t--frm-inp",
        "body": [

            "<div class=\"frmElm_${1}\">",

                "<label  for=\"\"> Demo label </label>",
                "<input  type=\"text\">",

            "</div>"

        ],
        "description": "Creates an input element."
    },



    "TcSS 1.0.0 Form Elements - Input 15":{
        "prefix": "t--frm-inp15",
        "body": [

            "<div class=\"frmElm_15\">",

                "<input type=\"text\" required>",
                "<label for=\"\"> ${1: Demo label} </label>",

            "</div>"

        ],
        "description": "Creates an input element, type-15."
    },



    "TcSS 1.0.0 Form Elements - Radio":{
        "prefix": "t--frm-radio",
        "body": [

            "<div>",
            "$0",
                "<label class=\"radio_${1}\" for=\"Demo1\">",
                    "<input type=\"radio\" id=\"Demo1\" name=\"1\" value=\"\">",
                    "<span></span> <!--Important-->",
                    "Demo 1",
                "</label>",
                "$0",
                "<label class=\"radio_${1}\" for=\"Demo2\">",
                    "<input type=\"radio\" id=\"Demo2\" name=\"1\" value=\"\">",
                    "<span></span> <!--Important-->",
                    "Demo 2",
                "</label>",
                "$0",
                "<label class=\"radio_${1}\" for=\"Demo3\">",
                    "<input type=\"radio\" id=\"Demo3\" name=\"1\" value=\"\">",
                    "<span></span> <!--Important-->",
                    "Demo 3",
                "</label>",
                "$0",
            "</div>"

        ],
        "description": "Creates an radio element."
    },



    "TcSS 1.0.0 Form Elements - Checkbox":{
        "prefix": "t--frm-check",
        "body": [

            "<div>",

                "<label class=\"check_${1}\" for=\"Demo1\">",
                    "<input type=\"checkbox\" id=\"Demo1\" name=\"1\" value=\"\">",
                    "<span></span> <!--Important-->",
                    "Demo 1",
                "</label>",

                "<label class=\"check_${1}\" for=\"Demo2\">",
                    "<input type=\"checkbox\" id=\"Demo2\" name=\"2\" value=\"\">",
                    "<span></span> <!--Important-->",
                    "Demo 2",
                "</label>",

                "<label class=\"check_${1}\" for=\"Demo3\">",
                    "<input type=\"checkbox\" id=\"Demo3\" name=\"3\" value=\"\">",
                    "<span></span> <!--Important-->",
                    "Demo 3",
                "</label>",

            "</div>"

        ],
        "description": "Creates an checkbox element."
    },



