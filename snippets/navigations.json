{
    "TcSS 1.0.0 Navigation - Non Sticky":{
        "prefix": "t--nsn",
        "body": [
            "<div class=\"nav\">",
                "$0",
                "\t<p class=\"txtLogo\">Logo</p>",
                "$0",
                "\t<form class=\"searchBar\" action=\"#\">",

                    "\t\t<input class=\"searchField\" type=\"text\">",
                    "\t\t<button class=\"searchBtn\" type=\"input\"><i class=\"fas fa-search\"></i></button>",

                "\t</form>",
                "$0",
                "\t<div class=\"mainLinks\">",

                    "\t\t<a class=\"mainLink\" href=\"#\">${1: main 1}</a>",

                    "\t\t<div class=\"dropDown\">",
                        "\t\t\t<a class=\"mainLink\" href=\"#\">main 2</a>",
                        "\t\t\t<div class=\"subLinks\">",
                            "\t\t\t\t<a class=\"subLink\" href=\"#\">sub 1</a>",
                        "\t\t\t</div>",
                    "\t\t</div>",

                    "\t\t<a class=\"mainLink\" href=\"#\">main 3</a>",

                    "\t\t<a class=\"hamburger\" href=\"#\"><i class=\"fas fa-bars\"></i></a>",

                "\t</div>",
                "$0",
            "</div>"
        ],
        "description": "Creates a non sticky navbar."
    },

